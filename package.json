{
  "name": "devtools-fm",
  "version": "2.2.0",
  "description": "The devtools module",
  "scripts": {
    "test": "node ../../scripts/run-tests",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "format:write": "prettier **/*.{json,md} --write",
    "format:test": "prettier **/*.{json,md} --list-different",
    "release:major": "release-it major",
    "release:minor": "release-it minor",
    "release:patch": "release-it patch"
  },
  "links": {
    "repository": "https://gitlab.com/ps-ide/fm/devtools",
    "bugs": "https://gitlab.com/ps-ide/fm/devtools/issues",
    "homepage": "https://wakanda.github.io/",
    "logo": "http://www.iconarchive.com/download/i75911/martz90/circle/settings.ico"
  },
  "keywords": [
    "PS",
    "module",
    "wakanda",
    "functional",
    "devtools",
    "postman",
    "controllers",
    "extractor"
  ],
  "contributors": [
    {
      "name": "Ahlam Benhachem",
      "email": "ahlam.benhachem@gmail.com"
    },
    {
      "name": "Basma El Yaagoubi",
      "email": "basma.elyaagoubi820@gmail.com"
    }
  ],
  "author": {
    "name": "Mohamed IDRISSI",
    "email": "med.idrissi@outlook.com"
  },
  "repository": {
    "type": "gitlab",
    "url": "https://gitlab.com/ps-ide/fm/devtools"
  },
  "license": "MIT",
  "dependencies": {
    "doctrine": "^3.0.0",
    "documentation": "^12.1.1",
    "esprima": "^4.0.1",
    "find-process": "^1.4.2",
    "func-loc": "^0.1.3",
    "libnpmpublish": "^1.1.2",
    "libnpmsearch": "^2.0.2",
    "react-dev-utils": "^9.0.3",
    "tar": "^4.4.10",
    "translate": "^1.1.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "eslint": "^6.2.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.3",
    "prettier": "^1.18.2",
    "release-it": "^12.3.5"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "post-merge": "npm i",
      "pre-commit": "lint-staged && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
